#!/usr/bin/env python3

import sys
import math

# Function to check if a number is prime
def is_prime(n):
    if n <= 1:
        return False
    if n <= 3:
        return True
    if n % 2 == 0 or n % 3 == 0:
        return False
    i = 5
    while i * i <= n:
        if n % i == 0 or n % (i + 2) == 0:
            return False
        i += 6
    return True

# Function to factorize a number into two smaller numbers
def factorize(num):
    n = int(num)
    for i in range(2, n // 2 + 1):
        if n % i == 0:
            if is_prime(i) and is_prime(n // i):
                return f"{n}={i}*{n//i}"
    return f"{n}={n}*1"

# Check for correct usage
if len(sys.argv) != 2:
    print("Usage: ./factors.py <file>")
    sys.exit(1)

file_name = sys.argv[1]

# Read numbers from the file and factorize them
with open(file_name, 'r') as file:
    for line in file:
        result = factorize(line.strip())
        print(result)